{
  "name": "@maptalks/gl",
  "version": "0.97.4",
  "description": "WebGL infrastrcture for maptalks",
  "main": "dist/maptalksgl.js",
  "module": "dist/maptalksgl.es.js",
  "scripts": {
    "build": "npm run lint && cd ../reshader.gl && npm run build && cd ../gl && rollup --environment BUILD:production -c rollup.config.js",
    "build-dev": "cd ../reshader.gl && npm run build-dev && cd ../gl && rollup -c rollup.config.js",
    "dev": "cd ../reshader.gl && npm run build-dev && cd ../gl && rollup -w -c rollup.config.js",
    "test": "cd ../layer-gltf && npm run build && cd ../gl && karma start --single-run",
    "tdd": "karma start karma.conf.dev.js --no-single-run",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.js",
    "pretest": "cd ../analysis && npm run build",
    "preversion": "npm test"
  },
  "files": [
    "dist/maptalksgl.js",
    "dist/maptalksgl.es.js",
    "dist/transcoders.js"
  ],
  "keywords": [
    "webgl",
    "maptalks"
  ],
  "author": "fuzhenn",
  "license": "UNLICENSED",
  "dependencies": {
    "@maptalks/fusiongl": "^0.6.13",
    "@maptalks/gltf-loader": "workspace:@maptalks/gltf-loader@*",
    "@maptalks/martini": "^0.4.0",
    "@maptalks/regl": "^3.3.5",
    "@maptalks/reshader.gl": "workspace:@maptalks/reshader.gl@*",
    "color": "^3.0.0",
    "colorin": "^0.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "eslint": "^8.57.0",
    "eslint-plugin-mocha": "^10.4.1",
    "expect.js": "^0.3.1",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "maptalks": "^1.0.0-rc.33",
    "mocha": "^10.3.0",
    "pixelmatch": "^4.0.2",
    "rollup": "^4.17.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.3.1"
  },
  "gitHead": "9eeea4807723d460fa7e09969c6556d53f6bed84"
}
